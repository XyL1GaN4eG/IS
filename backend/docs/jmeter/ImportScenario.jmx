          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="Generate persons import file">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="script">def suffix = ctx.getThreadNum() + '_' + System.currentTimeMillis()
def tmpDir = new File(System.getProperty('java.io.tmpdir'), 'is-jmeter')
tmpDir.mkdirs()
def f = new File(tmpDir, 'persons-' + suffix + '.yaml')
def yaml =
        'persons:\n' +
        '  - name: JMeter_' + suffix + '_A\n' +
        '    height: 170\n' +
        '    eyeColor: GREEN\n' +
        '    hairColor: BROWN\n' +
        '    nationality: ITALY\n' +
        '    coordinates:\n' +
        '      x: 10\n' +
        '      y: 25.5\n' +
        '    location:\n' +
        '      x: 1\n' +
        '      y: 2.2\n' +
        '      z: 3\n' +
        '      name: JM-Loc-' + suffix + '-A\n' +
        '  - name: JMeter_' + suffix + '_B\n' +
        '    height: 185\n' +
        '    eyeColor: ORANGE\n' +
        '    hairColor: WHITE\n' +
        '    nationality: INDIA\n' +
        '    coordinates:\n' +
        '      x: 20\n' +
        '      y: 30.0\n' +
        '    location:\n' +
        '      x: -5\n' +
