          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Race: count success/fail">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="script">def code = prev.getResponseCode()
def isOk = (code == '200' || code == '201')
def isDup = (code == '400' || code == '409')

if (isOk) {
  prev.setSuccessful(true)
  def id = vars.get('raceId')
  if (id) {
    synchronized(props) {
      if (props.get('raceCreatedId') == null) {
        props.put('raceCreatedId', id)
      }
    }
  }
} else if (isDup) {
  prev.setSuccessful(true)
} else {
  prev.setSuccessful(false)
  prev.setResponseMessage('Unexpected HTTP ' + code + ' ' + prev.getResponseDataAsString())
}

synchronized(props) {
  def success = (props.get('raceSuccessCount') ?: '0') as int
  def fail = (props.get('raceFailCount') ?: '0') as int
  if (isOk) {
    success++
  } else {
    fail++
  }
  props.put('raceSuccessCount', success.toString())
  props.put('raceFailCount', fail.toString())
}</stringProp>
